{
  "version": 2,
  "builds": [
    {
      "src": "*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
   "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "strict-origin-when-cross-origin",
          "value": "*"
        }
      ]
    }
  ]
}
